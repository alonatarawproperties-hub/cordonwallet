{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "ncarl.sigalat@icloud.com",
        "ascAppId": "6740627178"
      }
    }
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "distribution": "store",
        "autoIncrement": true
      },
      "android": {
        "distribution": "store",
        "autoIncrement": true
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_DOMAIN": "app.cordonwallet.com"
      }
    }
  }
}
